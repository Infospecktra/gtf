package org.yang.services.accountmgr;
import java.io.Serializable;

/**
 * @testcase org.test.org.yang.services.accountmgr.TestPermission 
 */
public class Permission  implements Serializable
{
   static final long serialVersionUID = 4711296382979765006L;

   private String id              = null;
   private String group_id        = null;
   private String service_id      = null;
   private String areasConcat     = null;
   private String operationsConcat= null;


   public Permission(){}

   public void setId(String pid){
           id = pid;
   }

   public void setGroupId(String gid){
           group_id = gid;
   }

   public void setServiceId(String sid){
           service_id = sid;
   }

   public void setAreasConcat(String areas){
           areasConcat = areas;
   }

   public void setOperationsConcat(String ops){
           operationsConcat = ops;
   }

   public String getId(){
       return this.id;
   }

   public String getGroupId(){
       return group_id;
   }

   public String getServiceId(){
       return service_id;
   }

   public String getAreasConcat(){
       return areasConcat;
   }

   public String getOperationsConcat(){
       return operationsConcat;
   }



public class DBConnectionCount { 
   
   public static int ConnectCount = 0;
   public static void printConnectionCount(String classMethodName)
   {
      //System.out.println("["+classMethodName+"]----------->ConnectCount="+ConnectCount);      	
   }	   	
   public static  int addConnCount()
   {
      return  ConnectCount++;	
   }	   	
   public static  int deductConnCount()
   {
      return  ConnectCount--;	
   }	   	
    
}

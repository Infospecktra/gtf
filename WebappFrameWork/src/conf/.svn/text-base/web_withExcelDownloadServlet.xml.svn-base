<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
  PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
  "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">

<web-app>

    <servlet>
	<servlet-name>JSPPrecompile</servlet-name>
        <servlet-class>org.yang.web.controller.JSPPrecompile</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet>
        <servlet-name>WF</servlet-name>
        <servlet-class>org.yang.web.controller.Controller</servlet-class>
	    <init-param>
	      <param-name>RequestManager</param-name>
	      <param-value>org.yang.web.controller.BaseRequestManager</param-value>
	    </init-param>
	    <init-param>
	      <param-name>configFileName</param-name>
	      <param-value>gtf</param-value>
	    </init-param>
        <load-on-startup>2</load-on-startup>
    </servlet>

    <servlet>
	  <servlet-name>fileUpload</servlet-name>
        <servlet-class>org.yang.customized.gtf.services.inventoryManager.FileUploadServlet</servlet-class>
	    <init-param>
	      <param-name>workingDirectory</param-name>
	      <param-value>C:\\GTFWorkingDir\\</param-value>
	    </init-param>
	    <init-param>
	      <param-name>upload-forward</param-name>
	      <param-value>http://localhost:80/wf/fileUpload</param-value>
	    </init-param>	    
	    <init-param>
	      <param-name>configFileName</param-name>
	      <param-value>fileUpload</param-value>
	    </init-param>
        <load-on-startup>3</load-on-startup>
    </servlet>  
   <servlet>
	  <servlet-name>xltDownload</servlet-name>
        <servlet-class>org.yang.customized.gtf.services.projectViewer.DownloadServlet</servlet-class>
	    <load-on-startup>4</load-on-startup>
    </servlet>  
    <servlet-mapping>
    	<servlet-name>fileUpload</servlet-name>
    	<url-pattern>/fileUpload/*</url-pattern>
    </servlet-mapping>
 
    <servlet-mapping>
    	<servlet-name>xltDownload</servlet-name>
    	<url-pattern>/xltDownload/*</url-pattern>
    </servlet-mapping>
	<servlet-mapping>
    	<servlet-name>WF</servlet-name>
    	<url-pattern>*.wf</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
    	<servlet-name>JSPPrecompile</servlet-name>
    	<url-pattern>*.precompile</url-pattern>
    </servlet-mapping>                  

    <session-config>
    	<session-timeout>30</session-timeout>
    </session-config>
    <mime-mapping>
    	<extension>pdf</extension>
    	<mime-type>application/pdf</mime-type>
    </mime-mapping>
    <welcome-file-list>
    	<welcome-file>index.htm</welcome-file>
    </welcome-file-list>
    <error-page>
    	<error-code>404</error-code>
    	<location>404.htm</location>
    </error-page>

  <!-- Application Tag Library Descriptor -->
  <taglib>
    <taglib-uri>/WEB-INF/wf.tld</taglib-uri>
    <taglib-location>/WEB-INF/wf.tld</taglib-location>
  </taglib>
</web-app>
